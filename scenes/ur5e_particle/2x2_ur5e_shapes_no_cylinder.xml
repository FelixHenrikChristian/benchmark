<mujoco model="2x2_ur5e_Shapes_NoCylinder">

  <include file="2x2_ur5e_container.xml"/>

  <size memory="1G"/>

  <option solver="CG" tolerance="1e-6" timestep=".01"/>

  <worldbody>
    <!-- Particle group 1 (red spheres) - above u0 at (-1, -1) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="-1.81 -1.81 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="sphere" size="0.05" condim="1" priority="1" rgba="0.8 0.2 0.1 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 2 (green boxes) - above u1 at (1, -1) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="0.19 -1.81 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="box" size="0.04 0.04 0.04" condim="1" priority="1" rgba="0.2 0.8 0.2 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 3 (blue ellipsoids) - above u2 at (-1, 1) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="-1.81 0.19 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="ellipsoid" size="0.04 0.03 0.05" condim="1" priority="1" rgba="0.2 0.4 0.9 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 4 (yellow capsules) - above u3 at (1, 1) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="0.19 0.19 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="capsule" size="0.03 0.04" condim="1" priority="1" rgba="0.9 0.8 0.2 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>
  </worldbody>

</mujoco>
