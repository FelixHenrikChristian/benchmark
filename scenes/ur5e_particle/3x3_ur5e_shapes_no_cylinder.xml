<mujoco model="3x3_ur5e_Shapes_NoCylinder">

  <statistic extent="6" meansize=".05"/>

  <size memory="2G"/>

  <option timestep="0.005" solver="CG" integrator="implicit"/>

  <visual>
    <rgba haze="0.15 0.25 0.35 1"/>
    <map shadowscale="0.5" zfar="40"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="64" height="64"/>
    <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>
    <material name="matplane" reflectance="0.3" texture="texplane" texrepeat="1 1" texuniform="true"/>
    <!-- Load ur5e model for attach -->
    <model name="ur5e" file="ur5e.xml"/>
  </asset>

  <default>
    <default class="wall">
      <geom type="plane" size="3 3 .05"/>
    </default>
  </default>

  <worldbody>
    <light directional="true" diffuse=".4 .4 .4" specular="0.1 0.1 0.1" pos="0 0 10" castshadow="false"/>
    <light diffuse=".6 .6 .6" specular="0.2 0.2 0.2" pos="0 0 8"/>

    <geom name="ground" type="plane" size="0 0 1" pos="0 0 0" quat="1 0 0 0" material="matplane" condim="1"/>

    <!-- 3x3 UR5e robotic arms using frame + attach -->
    <frame pos="-2 -2 0">
      <attach model="ur5e" body="base" prefix="u0_"/>
    </frame>
    <frame pos="0 -2 0">
      <attach model="ur5e" body="base" prefix="u1_"/>
    </frame>
    <frame pos="2 -2 0">
      <attach model="ur5e" body="base" prefix="u2_"/>
    </frame>
    <frame pos="-2 0 0">
      <attach model="ur5e" body="base" prefix="u3_"/>
    </frame>
    <frame pos="0 0 0">
      <attach model="ur5e" body="base" prefix="u4_"/>
    </frame>
    <frame pos="2 0 0">
      <attach model="ur5e" body="base" prefix="u5_"/>
    </frame>
    <frame pos="-2 2 0">
      <attach model="ur5e" body="base" prefix="u6_"/>
    </frame>
    <frame pos="0 2 0">
      <attach model="ur5e" body="base" prefix="u7_"/>
    </frame>
    <frame pos="2 2 0">
      <attach model="ur5e" body="base" prefix="u8_"/>
    </frame>

    <!-- Pit walls (6m x 6m) -->
    <geom name="+x" class="wall" zaxis="1 0 0"  pos="-3 0 -.25"/>
    <geom name="-x" class="wall" zaxis="-1 0 0" pos="3 0 -.25"/>
    <geom name="+y" class="wall" zaxis="0 1 0"  pos="0 -3 -.25"/>
    <geom name="-y" class="wall" zaxis="0 -1 0" pos="0 3 -.25"/>

    <!-- Particle group 1 (red spheres) - above u0 at (-2, -2) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="-2.81 -2.81 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="sphere" size="0.05" condim="1" priority="1" rgba="0.8 0.2 0.1 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 2 (green boxes) - above u1 at (0, -2) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="-0.81 -2.81 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="box" size="0.04 0.04 0.04" condim="1" priority="1" rgba="0.2 0.8 0.2 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 3 (blue ellipsoids) - above u2 at (2, -2) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="1.19 -2.81 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="ellipsoid" size="0.04 0.03 0.05" condim="1" priority="1" rgba="0.2 0.4 0.9 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 4 (yellow capsules) - above u3 at (-2, 0) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="-2.81 -0.81 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="capsule" size="0.03 0.04" condim="1" priority="1" rgba="0.9 0.8 0.2 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 5 (purple ellipsoids) - above u4 at (0, 0) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="-0.81 -0.81 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="ellipsoid" size="0.05 0.04 0.03" condim="1" priority="1" rgba="0.7 0.2 0.8 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 6 (cyan spheres) - above u5 at (2, 0) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="1.19 -0.81 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="sphere" size="0.05" condim="1" priority="1" rgba="0.2 0.8 0.9 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 7 (orange boxes) - above u6 at (-2, 2) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="-2.81 1.19 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="box" size="0.04 0.04 0.04" condim="1" priority="1" rgba="0.9 0.5 0.1 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 8 (pink capsules) - above u7 at (0, 2) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="-0.81 1.19 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="capsule" size="0.03 0.04" condim="1" priority="1" rgba="0.9 0.4 0.6 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>

    <!-- Particle group 9 (white spheres) - above u8 at (2, 2) -->
    <replicate count="10" offset=".18 0 0">
      <replicate count="10" offset="0 .18 0">
        <replicate count="10" offset="0 0 .18">
          <body pos="1.19 1.19 1.3">
            <joint type="slide" axis="1 0 0" group="3"/>
            <joint type="slide" axis="0 1 0" group="3"/>
            <joint type="slide" axis="0 0 1" group="3"/>
            <geom type="sphere" size="0.05" condim="1" priority="1" rgba="0.9 0.9 0.9 1"/>
          </body>
        </replicate>
      </replicate>
    </replicate>
  </worldbody>

</mujoco>
