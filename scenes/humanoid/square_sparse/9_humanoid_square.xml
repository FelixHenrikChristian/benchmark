<mujoco model="9 Humanoids Square">
  
  <option timestep="0.005" solver="CG" integrator="implicit"/>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1=".3 .5 .7" rgb2="0 0 0" width="512" height="512"/>
    <texture name="floor" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
    <material name="floor" texture="floor" texrepeat="1 1" texuniform="true" reflectance=".2"/>
    <model name="humanoid" file="humanoid.xml"/>
  </asset>

  <visual>
    <map force="0.1" zfar="100"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <quality shadowsize="4096"/>
    <global offwidth="1920" offheight="1080"/>
  </visual>

  <worldbody>
    <geom name="floor" size="12.25 12.25 .05" type="plane" material="floor" condim="3"/>
    <light directional="true" diffuse=".4 .4 .4" specular="0.1 0.1 0.1" pos="0 0 5" dir="0 0 -1" castshadow="false"/>
    <light name="spotlight" mode="targetbodycom" target="world" diffuse="1 1 1" specular="0.3 0.3 0.3" pos="0 0 10" cutoff="60"/>

    <frame pos="-1.5000 -1.5000 0">
      <attach model="humanoid" body="torso" prefix="u0_"/>
    </frame>
    <frame pos="-1.5000 0.0000 0">
      <attach model="humanoid" body="torso" prefix="u1_"/>
    </frame>
    <frame pos="-1.5000 1.5000 0">
      <attach model="humanoid" body="torso" prefix="u2_"/>
    </frame>
    <frame pos="0.0000 -1.5000 0">
      <attach model="humanoid" body="torso" prefix="u3_"/>
    </frame>
    <frame pos="0.0000 0.0000 0">
      <attach model="humanoid" body="torso" prefix="u4_"/>
    </frame>
    <frame pos="0.0000 1.5000 0">
      <attach model="humanoid" body="torso" prefix="u5_"/>
    </frame>
    <frame pos="1.5000 -1.5000 0">
      <attach model="humanoid" body="torso" prefix="u6_"/>
    </frame>
    <frame pos="1.5000 0.0000 0">
      <attach model="humanoid" body="torso" prefix="u7_"/>
    </frame>
    <frame pos="1.5000 1.5000 0">
      <attach model="humanoid" body="torso" prefix="u8_"/>
    </frame>
  </worldbody>
</mujoco>
